int cvSliceLength(CvSlice slice, CvSeq* seq) {
    void *fptr;
    int retval;

    fptr = dlsym(_Chcv_handle, "cvSliceLength_chdl");
    if(fptr == NULL) {
        fprintf(_stderr, "Error: %s(): dlsym(): %s\n", __func__,  dlerror());
        return -1;
    }
    dlrunfun(fptr, &retval, cvSliceLength, slice, seq);
    return retval;
}
