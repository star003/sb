void cvGetTextSize(char* text_string, CvFont* font, CvSize* text_size, int* baseline) {
    void *fptr;

    fptr = dlsym(_Chcv_handle, "cvGetTextSize_chdl");
    if(fptr == NULL) {
        fprintf(_stderr, "Error: %s(): dlsym(): %s\n", __func__,  dlerror());
        return;
    }
    dlrunfun(fptr, NULL, cvGetTextSize, text_string, font, text_size, baseline);
}
