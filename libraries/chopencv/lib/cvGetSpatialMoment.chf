double cvGetSpatialMoment(CvMoments* moments, int x_order, int y_order) {
    void *fptr;
    double retval;

    fptr = dlsym(_Chcv_handle, "cvGetSpatialMoment_chdl");
    if(fptr == NULL) {
        fprintf(_stderr, "Error: %s(): dlsym(): %s\n", __func__,  dlerror());
        return -1;
    }
    dlrunfun(fptr, &retval, cvGetSpatialMoment, moments, x_order, y_order);
    return retval;
}
